public class Server {

	private static final int port = 4700;
	private Socket socket;

	public static void main(String[] args) {
		try {
			ServerSocket serverSocketListener = new ServerSocket(port);
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
